{% extends "base.jinja" %}
{% block body %}
<div class="login-container">
	<h1>Login</h1>
	<form method="post" action="/login">
		Username <input type="text" name="username"><br>
		Password <input type="password" name="password"><br>
		<input type="submit" name="login">
	</form>
</div>
{% endblock %}